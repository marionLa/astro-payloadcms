---
import Layout from '../layouts/Layout.astro'
import ContactForm from '../components/ContactForm'
import type { Keys } from '../components/ContactForm'
const site_key = import.meta.env.CLOUDFARE_SITE_KEY
const payloadApiKey = import.meta.env.PAYLOAD_API_KEY

const keys: Keys = {
  payloadApiKey: payloadApiKey,
  cloudflareSiteKey: site_key,
}
---

<Layout>
  <ContactForm keys={keys} client:load />
</Layout>
